  "description": "Shares",
  "main": "./app/index.js",
  "dependencies": {
    "axios": "^0.8.1",
    "babel-polyfill": "^6.3.14",
    "electron": "^0.4.1",
    "hex-rgb": "^1.0.0",
    "history": "^1.17.0",
    "jsonp": "^0.2.0",
    "localforage": "^1.3.1",
    "moment": "^2.11.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "rgb-hex": "^1.0.0",
    "yahoo-finance-quotes": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "electron-packager": "^6.0.2",
    "electron-prebuilt": "^0.37.6",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-addons-test-utils": "^0.14.6",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "surge": "^0.17.7",
    "watchy": "^0.6.5",
    "webpack": "^1.12.11",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "clean:osx": "rimraf ./build/Shares-darwin-x64",
    "clean:win": "rimraf ./build/Shares-win32-x64",
    "compile": "webpack --progress --colors",
    "compile:watch": "webpack --progress --colors --watch",
    "electron": "electron .",
    "deploy": "webpack && surge app statae.com",
    "build:osx": "npm run clean:osx && cross-env NODE_ENV=development electron-packager ./app --overwrite --out='build' --icon='tomkp.icns' --platform=darwin --appname='Shares' --arch=x64",
    "build:win": "npm run clean:win && cross-env NODE_ENV=development electron-packager . --overwrite --out='build' --platform=win32 --appname='Shares' --arch=x64",
    "build": "npm run build:osx && npm run build:win"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
